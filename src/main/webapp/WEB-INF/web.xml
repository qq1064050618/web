<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
<servlet>
  <servlet-name>empServ</servlet-name>
  <servlet-class>com.moli.servlet.EmpServ</servlet-class>
</servlet>
<servlet-mapping>
  <!-- servlet-name 标签与 url-pattern 标签内容没有必要联系-->
  <servlet-name>empServ</servlet-name>
  <!--表示localhost后面的地址-->
  <url-pattern>/empServ</url-pattern>
</servlet-mapping>

  <welcome-file-list>
    <welcome-file>empServ</welcome-file>
  </welcome-file-list>
</web-app>

